{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "member-access": false,
        "quotemark": [true, "single", "jsx-double"],
        "no-implicit-dependencies": false,
        "no-namespace": false,
        "no-var-requires": false,
        "max-line-length": [true, 180],
        "no-submodule-imports": false,
        "no-console": false,
        "no-string-literal": false
    },
    "exclude": [
        "node_modules"
    ]
}